import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class GameWindow extends JFrame
{
	
	public GameWindow()
	{
		setTitle("Blackjack");
		setSize(500, 500);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		ImageIcon icon = new ImageIcon("Images/Clubs 1.png");
		setVisible(true);
	}
	
}
