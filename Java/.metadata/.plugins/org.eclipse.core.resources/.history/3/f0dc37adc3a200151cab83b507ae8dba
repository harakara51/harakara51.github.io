
public class pilot 
{
	private String name;
	private int experience;
	boolean isAirforce;

	public pilot(String name, int experience, boolean isAirforce)
	{
		
		this.name = name;
		this.experience = experience;
		this.isAirforce = isAirforce;
	} 
	/**
	 * @return the name
	 */
	public String getName()
	{
		return name;
	}
	/**
	 * @return the experience
	 */
	public int getExperience()
	{
		return experience;
	}
	/**
	 * @return the isAirforce
	 */
	public boolean isAirforce()
	{
		return isAirforce;
	}
	/**
	 * @param name
	 *            the name to set
	 */
	public void setName(String name)
	{
		this.name = name;
	}
	/**
	 * @param experience
	 *            the experience to set
	 */
	public void setExperience(int experience)
	{
		this.experience = experience;
	}
	/**
	 * @param isAirforce
	 *            the isAirforce to set
	 */
	public void setAirforce(boolean isAirforce)
	{
		this.isAirforce = isAirforce;
	}

}
