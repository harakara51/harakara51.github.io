package LoO;

public class GameMenu
{
    public void printGameMenu(NPCold player, NPCold monster, printMap map1)
    {
    
    	
   
       map1.drawMap();
        System.out.printf("Player Health: %1$d              Monster Health: %2$d", player.getHP(), monster.getHP());
        System.out.println("");
        
        
    }
}