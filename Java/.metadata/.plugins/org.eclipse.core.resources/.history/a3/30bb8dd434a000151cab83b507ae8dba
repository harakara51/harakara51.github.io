package examples;

public class Rectangle extends Shape
{

	private int width;
	private int height;
	private int area;
	
	public Rectangle()
	{
		super();
		this.width = 0;
		this.height = 0;
	}

	
	
	public Rectangle(int width, int height)
	{
		super();
		this.width = width;
		this.height = height;
	}
	
	@Override
	public void getArea()
	{
		area = width * height;

	}



}
